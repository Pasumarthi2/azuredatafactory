{
	"name": "df_SCD Type1 Demo",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_Blob_SourceDelimitedText_SCDType1",
						"type": "DatasetReference"
					},
					"name": "sourceBlobCSV",
					"description": "Read CSV files from Blob Container"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "assert1"
				},
				{
					"name": "alterRow1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Emp_No as string,",
				"          Emp_Name as string,",
				"          Department as string,",
				"          city as string,",
				"          LastUpdated_Date as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceBlobCSV",
				"alterRow1 assert() ~> assert1",
				"sourceBlobCSV alterRow() ~> alterRow1"
			]
		}
	}
}