{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SourceDelimitedText",
						"type": "DatasetReference"
					},
					"name": "SourceBlobContainer"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "TargetDelimitedText",
						"type": "DatasetReference"
					},
					"name": "sinkTargetBlobContainer"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SourceBlobContainer",
				"SourceBlobContainer sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     truncate: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkTargetBlobContainer"
			]
		}
	}
}